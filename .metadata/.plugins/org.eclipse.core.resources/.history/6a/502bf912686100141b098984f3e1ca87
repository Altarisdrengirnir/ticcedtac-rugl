package Game;


import java.awt.*;

import javax.swing.*;

public class Grid extends JPanel
{	
	ImageIcon X, O;
	int val = 0;
	
	
	//Constructor that creates a singel cell in our grid.
	public Grid() 
	{
		X = new ImageIcon(this.getClass().getResource("Ok-icon.png"));
		O = new ImageIcon(this.getClass().getResource("O.jpg"));
	}	
}
