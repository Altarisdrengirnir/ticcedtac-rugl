import java.awt.*;

import javax.swing.*;

import java.awt.*;
import java.awt.event.*;

import javax.swing.*;



public class tictactoe extends JFrame	
{
	JPanel p = new JPanel();
	Grid grid = new Grid();
	
	public tictactoe()
	{
		super("Altaris");
		setSize(400, 400);
		setResizable(false);
		setDefaultCloseOperation(EXIT_ON_CLOSE);
		p.setLayout(new GridLayout(3, 3));
		
			
		
		setVisible(true);
		
	}
	
	public static void main(String[] args)
	{
		new tictactoe();
	}
}
